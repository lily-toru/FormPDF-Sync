            gs.addErrorMessage('Failed to retrieve record information.');
            return;
        }
		
		var url = '/' + tableName + '.do?sys_id=' + sysId + '&sysparm_view=export_to_pdf&PDF';
            
        // 在新窗口打开PDF
        top.window.open(url, '_blank');
        
    } catch (error) {
        gs.logError('Error generating PDF: ' + error.message);
    }
}]]></script>

        <html><![CDATA[<?xml version="1.0" encoding="utf-8" ?>
<j:jelly xmlns:j="jelly:core">
<div style="padding: 15px;">
    <h3 style="margin-top:0">确认操作</h3>
    <p>确定要更新字段 <b>${gs.getMessage(sysparm_field_label)}</b> 吗？</p>
    
    <div style="text-align:right;margin-top:20px">
        <button class="btn btn-default" onclick="cancelAction()">取消</button>
        <button class="btn btn-primary" onclick="confirmAction()" style="margin-left:10px">确定</button>
    </div>
</div>

<script>
function confirmAction() {
    var ga = new GlideAjax('FieldUpdater');
    ga.addParam('sysparm_name', 'updateField');
    ga.addParam('sysparm_record_id', '${sysparm_record_id}');
    ga.addParam('sysparm_table_name', '${sysparm_table_name}');
    ga.addParam('sysparm_field_name', '${sysparm_field_name}');
    ga.addParam('sysparm_field_value', '${sysparm_field_value}');
    
    ga.getXMLAnswer(function(response) {
        if (response === 'success') {
            window.parent.g_form.setValue('${sysparm_field_name}', '${sysparm_field_value}');
            window.parent.GlideModal.get().destroy();
            window.parent.g_form.showFieldMsg('字段已更新', 'info');
        } else {
            alert('更新失败: ' + response);
        }
    });
}

function cancelAction() {
    window.parent.GlideModal.get().destroy();
}
</script>
</j:jelly>]]></html>

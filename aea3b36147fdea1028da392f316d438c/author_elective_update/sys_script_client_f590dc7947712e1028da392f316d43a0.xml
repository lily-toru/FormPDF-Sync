        <messages/>
        <name>AddDialogButton</name>
        <order/>
        <script><![CDATA[// Client Script: ShowConfirmationDialog
function onLoad() {
    if (typeof g_form != 'undefined') {
        addDialogButton();
    }
}

function addDialogButton() {
    var btn = document.createElement('button');
    btn.innerHTML = 'Update Field';
    btn.className = 'btn btn-default';
    btn.onclick = function(e) {
        e.preventDefault();
        showConfirmationDialog();
    };
    
    var header = document.querySelector('.form_header');
    if (header) {
        header.appendChild(btn);
    }
}

function showConfirmationDialog() {
    var dialog = new GlideDialogWindow('ConfirmationDialog', true);
    dialog.setTitle('Confirm Field Update');
    dialog.setSize(500, 200);
    dialog.render();
}]]></script>
